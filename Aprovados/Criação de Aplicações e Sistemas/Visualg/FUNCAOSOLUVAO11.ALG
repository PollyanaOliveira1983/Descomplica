Algoritmo "FuncaoSoluvao11"

funcao mult:inteiro
var aux:inteiro

inicio
//n,m e res sao variaveis globais
aux <- n * m
retorne aux
fimfuncao

var
n,m: inteiro
res:inteiro

inicio
escreval("Digite dois numeros: ")
leia(n)
leia(m)

res <- mult
escreva("O resultado da multiplicao é ",res)

Fimalgoritmo